#include <iostream> 
using namespace std; 
class Sample { 
public void printText1() 
{ 
 cout << "get in\n"; 
 } 
 void printText2() 
 { 
 cout << " get out\n"; 
 } 
 void printValue(int value) 
 { 
 cout << "value is: " << value << "\n"; 
 } 
} 
int main() 
{ 
 Sample obj; 
 obj.printText1(); 
 obj.printText2();
 obj.printValue(3); 
 return 0; 
} 
