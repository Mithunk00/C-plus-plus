class Student { 
public:  
 int rollNo;  
 string stdName;  
 float perc;  
} 
int main() 
{ 
 Student std; 
 std.rollNo = 24; 
 std.stdName = "Mithun Kundu "; 
 std.perc = 87.70f; 
 cout << "Student's Roll No.: " << std.rollNo << "\n";  cout << "Student's Name: " << std.stdName << "\n";  cout << "Student's Percentage: " << std.perc << "\n";
 return 0; 
} 
